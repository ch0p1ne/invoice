<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:viewmodels="clr-namespace:invoice.ViewModels"
                    xmlns:view="clr-namespace:invoice.Views">

    <DataTemplate DataType="{x:Type viewmodels:HomeVM}">
        <view:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:NewInvoiceVM}">
        <view:NewInvoiceView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewmodels:ExamenVM}">
        <view:ExamenView/>
    </DataTemplate>
</ResourceDictionary>